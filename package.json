{
  "name": "ngx-vcard-app",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build",
    "build:lib": "ng build --prod ngx-vcard && npm run copy-files",
    "test:lib:dev-debug": "ng test ngx-vcard --watch --browsers=ChromeDebugging",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e ",
    "commit": "git-cz",
    "copy-license": "copyfiles ./LICENSE ./dist/ngx-vcard",
    "copy-readme": "copyfiles ./README.md ./dist/ngx-vcard",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "test-docker": "npm run docker-build && docker run --rm build_vcard npm run test",
    "docker-build": "docker build -t build_vcard ."
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "ngx-vcard": "x",
    "rxjs": "6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "commitizen": "4.0.3",
    "copyfiles": "2.2.0",
    "cz-conventional-changelog": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "^9.0.0",
    "protractor": "5.4.3",
    "puppeteer": "2.1.1",
    "semantic-release": "17.0.4",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "typescript": "3.8.3",
    "webdriver-manager": "12.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielHabenicht/ngx-vcard.git"
  },
  "release": {
    "pkgRoot": "dist/ngx-vcard"
  }
}
