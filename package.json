{
  "name": "ngx-vcard-app",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build",
    "build:lib": "ng build --prod ngx-vcard && npm run copy-files",
    "test:lib:dev-debug": "ng test ngx-vcard --watch --browsers=ChromeDebugging",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e ",
    "commit": "git-cz",
    "copy-license": "cp ./LICENSE ./dist/ngx-vcard",
    "copy-readme": "cp ./README.md ./dist/ngx-vcard",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "core-js": "^3.0.0",
    "ngx-vcard": "^1.1.3",
    "rxjs": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.1",
    "@angular-devkit/build-ng-packagr": "~0.13.1",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~11.13.0",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ng-packagr": "^5.0.1",
    "protractor": "~6.0.0",
    "semantic-release": "^15.10.5",
    "ts-node": "~8.0.3",
    "tsickle": ">=0.33.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "travis-deploy-once": "^5.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielHabenicht/ngx-vcard.git"
  },
  "release": {
    "pkgRoot": "dist/ngx-vcard"
  }
}
